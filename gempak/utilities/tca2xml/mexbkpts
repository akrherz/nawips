
tac ${1-tcabkptlz.tbl} | awk ' { 
        if ($9 == '20' ) printf "</path>\n        </segment>\n        <segment>\n            <breakpoint name=""\042"$3"\042"" state=""\042"$4"\042"" country=""\042"$5"\042"" location=""\042"$7/100","$6/100"\042"" official=\042true\042/>\n            <path>" 
        if ($7 == '25') printf " "$5/100","$4/100
                       } 
        END{ printf "</path>\n        </segment>\n" } 
' | sed '/<path></d' | sed 's/<path> /<path>/' | tail -n+3
