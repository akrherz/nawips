#
#  GNUmakefile_subdir
#
#  Use this in directories that ONLY contain
#	subdirectories; no local processing and
#	not a root (top directory) for a library.
#
SHELL = /bin/sh

include $(GEMINC)/subdirs.mk

check::
	@echo ""
	@echo "SUBPATH = $(SUBPATH)"
	@echo "SUBDIRS = $(SUBDIRS)"
	@echo "TARGETS = $(TARGETS)"
	@echo ""

.PHONY: check
